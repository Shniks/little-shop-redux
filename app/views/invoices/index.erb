  <html>
  <head>
    <title>Invoices - Little Shop Redux</title>
  </head>
  <body>
  <% invoices.each do |invoice| %>
  <ul>
    <li>Id / <%= invoice.id %></li>
    <li>Status / <%= invoice.status %></li>
    <li>Created-at / <%= invoice.created_at %></li>
    <li>Updated-at / <%= invoice.updated_at %></li>
  </ul>
  <% end %>
</body>
</html>
